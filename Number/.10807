#include <iostream>

using namespace std;

int main()
{
    int a,c,N = 0;
    cin >> a;
    int b[a] = {0,};
    
    for(int i=0;i<a;i++)
        cin >> b[i] ;
    
    cin >> c;
    
    for(int i=0;i<a;i++){
        if(b[i] == c)
            N++;
    }
    cout << N <<endl;
    
    return 0;
}
