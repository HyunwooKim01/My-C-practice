#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    
    vector<string> v = {"아이스티","카페라떼","하이볼"};
    
    v.push_back("아아");
    v.push_back("몬스터");
    for (auto n : v)    cout << n << " ";
    
    v.pop_back();
    
    cout << endl;
    cout << v.size() << endl;
    
    v.push_back("몬스터");
    
    for (auto n : v)    cout << n << " ";
    
    cout << endl;
    cout << v.size() << endl;
    
}
