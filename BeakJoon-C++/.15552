#include <iostream>
using namespace std;

int main()
{
    cin.tie(NULL); cout.tie(NULL); ios::sync_with_stdio(false);
    
    int a, b, c = 0;
    
    cin >> a;
    
    int result[a] = {0};
    
    for(int i=0;i<a;i++){
        cin >> b >> c;
        result[i] = b+c;
        cout << result[i] <<"\n";
    }

    return 0;
}
