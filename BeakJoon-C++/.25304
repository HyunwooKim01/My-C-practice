#include <iostream>
using namespace std;

int main()
{
    int a, b, c, d, result = 0;
    
    cin >> a;
    cin >> b;
    
    for(int i=0; i<b; i++){
        cin >> c >> d;
        result += c * d;
    }
    
    if(a == result)
        cout << "Yes" << endl;
    else
        cout << "No" << endl;
    
    return 0;
}
